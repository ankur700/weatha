{
  "manifest_version": 3,
  "name": "Weatha",
  "version": "0.0.1",
  "description": "A simple weather web extention to display current weather conditions.",
  "permissions": ["geolocation"],
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon32.png",
    "128": "images/icon128.png"
  }
}